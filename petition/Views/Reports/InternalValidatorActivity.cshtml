@{
    Layout = "_AdminLayout";
}
@model createBatchVM

<div class="content-wrapper">
    <div class="content-body">
        <!-- Basic Vertical form layout section start -->
        <section id="basic-vertical-layouts">
            <div class="row">
                <div class="col-12">
                    <div class="card h-100">
                        <div class="card-header">
                            <h4 class="card-title">
                                Validator Activity
                            </h4>
                        </div>
                        <div class="card-body">
                           
                            <div class="row">
                                <div class="col-lg-12 col-xl-10 offset-xl-1">
                                    <div class="row">
                                        <div class="col-sm-4 mt-1">
                                            <div class="row">
                                                <div class="col-4">
                                                    <label class="float-right text-right mt-1">
                                                        Select :
                                                    </label>
                                                </div>
                                                <div class="col-8">
                                                    <select class="form-control petition" id="basicSelect">
                                                    </select>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-sm-4 mt-1">
                                            <div class="row">
                                                <div class="col-4">
                                                    <label class="float-right text-right mt-1">
                                                        Start Date :
                                                    </label>
                                                </div>
                                                <div class="col-8">
                                                    <input type="text" id="fp-default" class="date form-control flatpickr-basic bg-white" placeholder="YYYY-MM-DD" />
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-sm-4 mt-1">
                                            <div class="row">
                                                <div class="col-4">
                                                    <label class="float-right text-right mt-1">
                                                        End Date :
                                                    </label>
                                                </div>
                                                <div class="col-8">
                                                    <input type="text" id="fp-default" class="date form-control flatpickr-basic bg-white" placeholder="YYYY-MM-DD" />
                                                </div>
                                            </div>
                                        </div>

                                        <div class="col-12 mt-1">
                                            <button type="button" id="Submit" class="btn btn-sm btn-primary float-right">Go</button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                           
                        </div>
                    </div>
                </div>
            </div>

        </section>
        <!-- Basic Vertical form layout section end -->
    </div>
</div>